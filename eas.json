{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk"
      }
    },
    "agent-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_AGENT_ID": "$EXPO_PUBLIC_AGENT_ID",
        "EXPO_PUBLIC_AGENT_NAME": "$EXPO_PUBLIC_AGENT_NAME",
        "EXPO_PUBLIC_BUILD_ID": "$EXPO_PUBLIC_BUILD_ID"
      }
    }
  }
}