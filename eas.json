{
  "cli": {
    "version": ">= 16.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "agent-apk": {
      "extends": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      },
      "env": {
        "EXPO_PUBLIC_AGENT_ID": "secret:EXPO_PUBLIC_AGENT_ID",
        "EXPO_PUBLIC_AGENT_NAME": "secret:EXPO_PUBLIC_AGENT_NAME"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
